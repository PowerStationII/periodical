    						<div class="input_con_type_top wid190 margt4">
    							<input type="text" value="$!{query.status}" id="status" name="status" class="wid190 ip_input_con">
    						</div>
    					</div>
    					<div class="left wid280 margt10 rel_pos paddingl15">
    						<label class="label_txt wid80 txtright margr8 margt11 paddingl0">：</label>
    						<div class="input_con_type_top wid190 margt4">
    							<input type="text" value="$!{query.extend1}" id="extend1" name="extend1" class="wid190 ip_input_con">
    						</div>
    					</div>
    					<div class="left wid280 margt10 rel_pos paddingl15">
    						<label class="label_txt wid80 txtright margr8 margt11 paddingl0">：</label>
    						<div class="input_con_type_top wid190 margt4">
