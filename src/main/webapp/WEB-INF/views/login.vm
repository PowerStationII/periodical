<!DOCTYPE html>
<html>
<head>
<title>用户登录</title>
<meta http-equiv="X-UA-Compatible" content="IE=edge" />
<meta http-equiv="content-type" content="text/html; charset=utf-8" />
<meta name="apple-mobile-web-app-capable" content="yes" />
<link href="css/axure_rp_page.css" type="text/css" rel="stylesheet" />
<link href="css/data_styles.css" type="text/css" rel="stylesheet" />
<link href="css/login_styles.css" type="text/css" rel="stylesheet" />
<script type="text/javascript" src="js/jquery-1.11.3.js"></script>
</head>
<script type="text/javascript">
	function submitForm() {
		if ($('#u34_input').val() == "" || $('#u35_input').val() == "") {
			alert('请输入用户名和密码');
			return;
		} else {
			$('#sub').submit();
		}
	}
</script>


<body>
	<form action="toLogin" method="POST" id="sub" name="sub">
		<!-- Unnamed (Shape) -->
		<div id="u18" class="ax_paragraph">
			<img id="u18_img" class="img" src="img/portal/transparent.gif" />
			<!-- Unnamed () -->
			<div id="u19" class="text">
				<p>
					<span>邮箱</span><span>：</span>
				</p>
			</div>
		</div>

		<!-- name (Text Field) -->
		<div id="u34" class="ax_text_field" data-label="name">
			<input id="u34_input" type="text" name="email" value="" />
		</div>

		<!-- Unnamed (Shape) -->
		<div id="u22" class="ax_paragraph">
			<img id="u22_img" class="img " src="img/portal/transparent.gif" />
			<!-- Unnamed () -->
			<div id="u23" class="text">
				<p>
					<span>密码：</span>
				</p>
			</div>
		</div>

		<!-- password (Text Field) -->
		<div id="u35" class="ax_text_field" data-label="password">
			<input id="u35_input" name="password" type="password" value="" />
		</div>

		<input type="hidden" name="systemId" value=${systemId}></input>

		<!-- Unnamed (Shape) -->
		<div id="u24" class="ax_shape">
			<img id="u24_img" class="img " src="img/login/u24.png" />
			<!-- Unnamed () -->
			<div id="u25" class="text">
				<p>
					<a href="javascript:;" onclick="submitForm(this)">登录</a>
				</p>
			</div>
		</div>
	</form>


	<!-- Unnamed (Shape) -->
	<div id="u28" class="ax_paragraph">
		<img id="u28_img" class="img " src="img/portal/transparent.gif" />
		<!-- Unnamed () -->
		<div id="u29" class="text">
			<p>
				<a href="toRegister?systemId=${systemId}&roleId=${roleId}"
					style='text-decoration: none;'><span>注册</span></a>
			</p>
		</div>
	</div>

	<!-- Unnamed (Shape) -->
	<div id="u30" class="ax_paragraph">
		<img id="u30_img" class="img " src="img/portal/transparent.gif" />
		<!-- Unnamed () -->
		<div id="u31" class="text">
			<p>
				<span>|</span>
			</p>
		</div>
	</div>

	<!-- Unnamed (Shape) -->
	<div id="u32" class="ax_paragraph" title="找回密码">
		<img id="u32_img" class="img " src="img/portal/transparent.gif" />
		<!-- Unnamed () -->
		<div id="u33" class="text">
			<p>
				<span>忘记密码？</span>
			</p>
		</div>
	</div>
</body>
</html>
