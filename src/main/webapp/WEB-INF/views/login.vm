  		$('#u4').click(function(){
  			$('#login').hide();
  			$('iframe').remove();
  			$('#back').remove();
  			$('#u12').after(frame.replace('totototo','toTouGao'));
  			$('#back').css('margin-top','740px');
  		});
  		$('#u6').click(function(){
  			$('#login').hide();
