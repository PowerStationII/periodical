  		});
  		$('#u12').click(function(){
  			$('#login').hide();
  			$('iframe').remove();
  			$('#back').remove();
  			$('#u12').after(frame.replace('totototo','toLianXi'));
  			$('#back').css('margin-top','640px');
  		});
	});
	
	function reBack () {
		$('#login').show();
		$('iframe').remove();
		$('#back').remove();
	}
  </script>


