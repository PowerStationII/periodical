<body>
	<div id="articleInfos">
		<input type="hidden" name="userId" value="${userId}">
		<table style="border: 1px solid black;" rules="all">
			<tr>
				<td>标题</td>
				<td>作者</td>
				<td>投稿时间</td>
				<td>操作</td>
			</tr>
			#foreach($editorArticleDealRespDto in ${list})   
			<tr>
				<td>${editorArticleDealRespDto.articleInfo.articleCnTitle}</td>
				<td>${editorArticleDealRespDto.authorInfos[0].authorName}</td>
				<td>${editorArticleDealRespDto.articleInfo.articleCnTitle}</td>
				<td><a href="../author/toArticleQueryDetailPage?artilceId=${editorArticleDealRespDto.articleInfo.id}">${editorArticleDealRespDto.articleInfo.articleCnTitle}</a></td>
			</tr>
			#end
		</table>
		<br>
	</div>
